{
    "name": "MyGlamm Extension",
    "version": "1.0",
    "description": "Build an MyGlamm Extension with Angular",
    "manifest_version": 2,
    "permissions": [
        "activeTab",
        "webNavigation"
      ],
      "background": {
          "scripts": [
              "background.js",
              "runtime.js"
          ]
      },
    "browser_action": {
        "default_popup": "index.html",
        "default_icon": "./assets/favicon.png"
    }
}